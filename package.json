{
  "name": "labs-angular-backend",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "typings": "lib/index.d.js",
  "scripts": {
    "build": "npm run tsc:project",
    "coverage": "npm run test-cov && npm run test-remap",
    "postinstall": "npm run build",
    "tslint": "tslint ./*.ts ./src/*.ts",
    "test": "mocha --reporter spec --check-leaks --bail test/",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "tsc:project": "tsc --project tsconfig.json",
    "start": "concurrently \"npm run tsc:w\" \"nodemon app.js --ignore *.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NodeAndTyped/labs-angular2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NodeAndTyped/labs-angular2/issues"
  },
  "homepage": "https://github.com/NodeAndTyped/labs-angular2#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "install": "^0.8.8",
    "method-override": "^2.3.8",
    "morgan": "^1.8.1",
    "socket.io": "^1.7.3",
    "ts-express-decorators": "^1.4.6",
    "ts-json-properties": "^1.1.4"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/socket.io": "^1.4.29",
    "concurrently": "^3.4.0",
    "nodemon": "^1.11.0"
  }
